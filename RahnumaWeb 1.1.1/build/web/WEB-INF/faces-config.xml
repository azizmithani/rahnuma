<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="1.2" 
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
    <managed-bean>
        <managed-bean-name>SessionBean1</managed-bean-name>
        <managed-bean-class>rahnumaweb.SessionBean1</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ApplicationBean1</managed-bean-name>
        <managed-bean-class>rahnumaweb.ApplicationBean1</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>RequestBean1</managed-bean-name>
        <managed-bean-class>rahnumaweb.RequestBean1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Rahnuma</managed-bean-name>
        <managed-bean-class>rahnumaweb.Rahnuma</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>RahnumaParameters</managed-bean-name>
        <managed-bean-class>rahnumaweb.RahnumaParameters</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/Rahnuma.jsp</from-view-id>
        <navigation-case>
            <from-outcome>Parameters</from-outcome>
            <to-view-id>/RahnumaParameters.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>RahnumaSubmitted</managed-bean-name>
        <managed-bean-class>rahnumaweb.RahnumaSubmitted</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/RahnumaParameters.jsp</from-view-id>
        <navigation-case>
            <from-outcome>Back To Main</from-outcome>
            <to-view-id>/Rahnuma.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Submitted</from-outcome>
            <to-view-id>/RahnumaSubmitted.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/RahnumaSubmitted.jsp</from-view-id>
        <navigation-case>
            <from-outcome>Restart</from-outcome>
            <to-view-id>/Rahnuma.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>RahnumaLogin</managed-bean-name>
        <managed-bean-class>rahnumaweb.RahnumaLogin</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/RahnumaLogin.jsp</from-view-id>
        <navigation-case>
            <from-outcome>ValidLogin</from-outcome>
            <to-view-id>/Rahnuma.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>RahnumaStart</managed-bean-name>
        <managed-bean-class>rahnumaweb.RahnumaStart</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/RahnumaStart.jsp</from-view-id>
        <navigation-case>
            <from-outcome>Start</from-outcome>
            <to-view-id>/Rahnuma.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
